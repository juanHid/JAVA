<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Servlet1</servlet-name>
        <servlet-class>controller.Servlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Total_invertido</servlet-name>
        <servlet-class>controller.Total_invertido</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>controller.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Total</servlet-name>
        <servlet-class>controller.Total</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Minimos</servlet-name>
        <servlet-class>controller.Minimos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ordenar</servlet-name>
        <servlet-class>controller.Ordenar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Buscar</servlet-name>
        <servlet-class>controller.Buscar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/Servlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Total_invertido</servlet-name>
        <url-pattern>/Total_invertido</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Total</servlet-name>
        <url-pattern>/Total</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Minimos</servlet-name>
        <url-pattern>/Minimos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ordenar</servlet-name>
        <url-pattern>/Ordenar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Buscar</servlet-name>
        <url-pattern>/Buscar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
